{
    "name": "media-alchemyst/media-alchemyst",
    "type": "library",
    "description": "An Object Oriented wrapper for easy multimedia conversion, based on Imagine, FFMpeg, SwfTools, Unoconv and other libs",
    "keywords": ["video processing", "video", "audio processing", "audio", "image", "image processing"],
    "license": "MIT",
    "authors": [
        {
            "name": "Romain Neutron",
            "email": "imprec@gmail.com",
            "homepage": "http://www.lickmychip.com/"
        },
        {
            "name": "Phraseanet Team",
            "email": "info@alchemy.fr",
            "homepage": "http://www.phraseanet.com/"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/avalanche123/Imagine"
        },
        {
            "type": "vcs",
            "url": "https://github.com/jygaulier/Imagine-Silex-Service-Provider"
        },
        {
            "type": "vcs",
            "url": "https://github.com/jygaulier/PHPSwftools"
        }
    ],
    "require": {
        "php": "~7.0",
        "alchemy/ghostscript": "~0.4.0",
        "alchemy/mediavorus": "~2.0",
        "imagine/imagine": "~0.7",
        "monolog/monolog": "~1.0",
        "neutron/temporary-filesystem": "^2.1.1",
        "php-ffmpeg/php-ffmpeg": "~2.0",
        "php-mp4box/php-mp4box": "~0.3.0",
        "php-unoconv/php-unoconv": "~0.3.0",
        "pimple/pimple": "~3.0",
        "swftools/swftools": "~2.0",
        "symfony/console": "^2.1|^3.0",
        "symfony/filesystem": "^2.1|^3.0",
        "symfony/process": "^2.1.1|^3.0"
    },
    "require-dev": {
        "alchemy/phpexiftool": "^0.4.0|^0.5.0",
        "neutron/silex-imagine-provider": "2.0",
        "phpunit/phpunit": "^4.1|^5.0",
        "silex/silex": "~2.0"
    },
    "autoload": {
        "psr-0": {
            "MediaAlchemyst": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.5.x-dev"
        }
    }
}
